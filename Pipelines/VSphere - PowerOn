pipeline{
    agent { label 'linux' }
    parameters {
    string(name: 'VM', defaultValue: 'VMS-Milestone-20xx')
    }
    stages{
        stage('Power-On-VM'){
            steps{
                vSphere buildStep: [$class: 'PowerOn', timeoutInSeconds: 0, vm: "${params.VM}"], serverName: 'vCenter-58'
            }
        }
    }
}
